{
  "name": "psd2-onboarding",
  "version": "1.0.0",
  "description": "sample app for building jwt request",
  "repository": "electron/simple-samples",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "lint": "xo",
    "test": "npm run lint",
    "start": "electron . --remote-debugging-port=9223",
    "pack": "electron-builder --dir",
    "dist": "electron-builder --macos --linux --windows",
    "release": "np"
  },
  "license": "ISC",
  "dependencies": {
    "electron-debug": "^3.2.0",
    "electron-store": "^8.0.0",
    "electron-unhandled": "^3.0.2",
    "electron-updater": "^4.3.8",
    "electron-util": "^0.15.1"
  },
  "devDependencies": {
    "electron": "^12.0.6",
    "electron-builder": "^22.10.5",
    "np": "^7.5.0",
    "xo": "^0.39.1"
  },
  "author": {
    "name": "Rajesh Manoharan",
    "email": "sdsadas.com",
    "url": "http://sdassaa.com/"
  },
  "xo": {
    "envs": [
      "node",
      "browser"
    ]
  },
  "np": {
    "publish": false,
    "releaseDraft": false
  },
  "build": {
    "appId": "com.company.AppName",
    "mac": {
      "category": "public.app-category.social-networking",
      "darkModeSupport": true
    },
    "dmg": {
      "iconSize": 160,
      "contents": [
        {
          "x": 180,
          "y": 170
        },
        {
          "x": 480,
          "y": 170,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "category": "Network;Chat"
    }
  }
}
